{
  "name": "react-viewport-utils-dev-tools",
  "version": "1.0.0",
  "description": "Debugger for the react-viewport-utils library",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist/chrome-extension",
    "build": "parcel build --out-dir=dist/chrome-extension lib/chrome-extension/panel.html lib/chrome-extension/devtools.html lib/chrome-extension/background.ts && parcel build --experimental-scope-hoisting lib/chrome-extension/content_script.ts --out-dir=dist/chrome-extension",
    "postbuild": "cp lib/chrome-extension/manifest.json dist/chrome-extension/manifest.json"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/garthenweb/react-viewport-utils"
  },
  "bugs": {
    "url": "https://github.com/garthenweb/react-viewport-utils/issues"
  },
  "author": "Jannick Garthen <jannick.garthen@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^10.12.30",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "@types/styled-components": "^4.1.12",
    "parcel": "^1.12.0",
    "typescript": "^3.3.3333"
  }
}
