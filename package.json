{
  "name": "react-viewport-utils",
  "version": "1.10.0",
  "description": "Utility components for working with the viewport in react",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "rm -rf ./.cache && parcel ./examples/index.html --out-dir='examples-dist'",
    "precompile": "rm -rf dist",
    "compile": "rollup -c",
    "test": "jest",
    "fmt": "prettier --write \"lib/*.{ts,tsx}\"  \"examples/*.{ts,tsx}\"",
    "prepublish": "npm run compile",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/garthenweb/react-viewport-utils"
  },
  "bugs": {
    "url": "https://github.com/garthenweb/react-viewport-utils/issues"
  },
  "engines": {
    "node": "^8.9.0 || >= 10.13.0"
  },
  "author": "Jannick Garthen <jannick.garthen@gmail.com>",
  "keywords": [
    "react",
    "viewport",
    "scroll",
    "dimensions",
    "size",
    "resize",
    "event",
    "observer",
    "window",
    "screen"
  ],
  "license": "MIT",
  "devDependencies": {
    "@testing-library/react": "^9.1.3",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.3",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "concurrently": "^4.1.2",
    "coveralls": "^3.0.6",
    "jest": "^24.9.0",
    "parcel-bundler": "1.12.3",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rollup": "^1.20.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript2": "^0.24.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "memoize-one": "^5.1.1"
  },
  "peerDependencies": {
    "react": "^16.3.0"
  }
}
