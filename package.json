{
  "name": "react-viewport-utils",
  "version": "0.3.3",
  "description": "Utility components for working with the viewport in react",
  "main": "dist/index.js",
  "scripts": {
    "start": "rm -rf ./.cache && parcel ./examples/index.html --out-dir='examples-dist'",
    "precompile": "rm -rf dist",
    "compile": "tsc --project ./tsconfig.json",
    "test": "jest lib",
    "fmt": "prettier --write \"lib/*.{ts,tsx}\"  \"examples/*.{ts,tsx}\"",
    "prepublish": "npm run compile",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/garthenweb/react-viewport-utils"
  },
  "bugs": {
    "url": "https://github.com/garthenweb/react-viewport-utils/issues"
  },
  "author": "Jannick Garthen <jannick.garthen@gmail.com>",
  "keywords": [
    "react",
    "viewport",
    "scroll",
    "dimensions",
    "size"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/memoize-one": "^3.1.1",
    "@types/node": "^10.9.2",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/recompose": "^0.26.4",
    "concurrently": "^4.0.0",
    "jest": "^23.5.0",
    "parcel-bundler": "1.9.4",
    "prettier": "^1.14.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "memoize-one": "^4.0.2",
    "raf": "^3.4.0",
    "recompose": "^0.30.0"
  },
  "peerDependencies": {
    "react": "^16.3.0"
  }
}
