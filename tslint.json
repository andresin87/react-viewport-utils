{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "max-classes-per-file": false,
    "jsx-no-lambda": false,
    "no-console": false,
    "object-literal-sort-keys": false
  },
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts"
    ]
  }
}
